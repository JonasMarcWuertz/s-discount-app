{
	"masterPicture": "./SPA",
	"iconsPath": "/",
	"design": {
		"ios": {
			"pictureAspect": "noChange",
			"assets": {
				"ios6AndPriorIcons": true,
				"ios7AndLaterIcons": true,
				"precomposedIcons": true,
				"declareOnlyDefaultIcon": true
			}
		},
		"desktopBrowser": {
			"design": "raw"
		},
		"windows": {
			"pictureAspect": "noChange",
			"backgroundColor": "#da532c",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": true,
				"windows10Ie11EdgeTiles": {
					"small": true,
					"medium": true,
					"big": true,
					"rectangle": true
				}
			}
		},
		"androidChrome": {
			"pictureAspect": "noChange",
			"themeColor": "#ffffff",
			"manifest": {
				"name": "S-Rabatt-App",
				"display": "standalone",
				"orientation": "notSet",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": true,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "blackAndWhite",
			"threshold": 91.5625,
			"themeColor": "#5bbad5"
		}
	},
	"settings": {
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false,
		"readmeFile": true,
		"htmlCodeFile": true,
		"usePathAsIs": false
	}
}